journal = {The FEBS journal},
keywords = {Animals,Cells, Cultured,Dual Specificity Phosphatase 6,Dual Specificity Phosphatase 6: physiology,Extracellular Signal-Regulated MAP Kinases,Extracellular Signal-Regulated MAP Kinases: metabo,Feedback, Physiological,Fibroblasts,Fibroblasts: metabolism,Genes, ras,Humans,MAP Kinase Signaling System,MAP Kinase Signaling System: physiology,Mice,Models, Biological,Oligonucleotide Array Sequence Analysis,Phosphorylation,RNA Stability,RNA, Messenger,RNA, Messenger: metabolism,Rats,Signal Transduction,Signal Transduction: physiology,Systems Biology},
