journal = {Molecular systems biology},
keywords = {Biological Clocks,Biological Clocks: physiology,Cell Line, Tumor,DNA Damage,Dose-Response Relationship, Radiation,Feedback, Physiological,Female,Gamma Rays,Gamma Rays: adverse effects,Humans,Models, Theoretical,Proto-Oncogene Proteins c-mdm2,Proto-Oncogene Proteins c-mdm2: physiology,Tumor Suppressor Protein p53,Tumor Suppressor Protein p53: metabolism,Tumor Suppressor Protein p53: physiology},
month = jan,
