issn = {10960929},
journal = {Toxicological Sciences},
month = may,
number = {1},
pages = {69--77},
title = {{A Rapid and Sensitive Reporter Gene that Uses Green Fluorescent Protein Expression to Detect Chemicals with Estrogenic Activity}},
