issn = {1208-6002},
journal = {Biochemistry and cell biology = Biochimie et biologie cellulaire},
keywords = {axon,axon growth,centrosome,fission yeast,laser ablation,meiosis,neurons,nuclear oscillations,schizosaccharomyces pombe,spindle pole body,two-photon},
month = dec,
number = {6},
pages = {554--61},
pmid = {22077425},
title = {{Schizosaccharomyces pombe, unlike Saccharomyces cerevisiae, may not directly regulate nuclear-cytoplasmic transport of spliced tRNAs in response to nutrient availability.}},
