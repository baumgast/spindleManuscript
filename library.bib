keywords = {Animals,Biocatalysis,Escherichia coli,Escherichia coli Proteins,Escherichia coli Proteins: metabolism,Escherichia coli: enzymology,Escherichia coli: metabolism,Glucaric Acid,Glucaric Acid: metabolism,Metabolic Networks and Pathways,Mevalonic Acid,Mevalonic Acid: metabolism,Mice,Protein Binding,Protein Engineering,Rats,Titrimetry},
month = aug,
