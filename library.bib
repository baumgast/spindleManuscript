issn = {1558-0062},
journal = {IEEE transactions on medical imaging},
keywords = {Algorithms,Hela Cells,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Microscopy, Video,Microscopy, Video: methods,Normal Distribution,Poisson Distribution},
month = feb,
number = {2},
pages = {442--54},
pmid = {19900849},
title = {{Patch-based nonlocal functional for denoising fluorescence microscopy image sequences.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19900849},
volume = {29},
year = {2010}
}
