keywords = {Biological Clocks,Biological Clocks: genetics,Biological Clocks: physiology,Escherichia coli,Escherichia coli: cytology,Escherichia coli: genetics,Escherichia coli: physiology,Feedback, Physiological,Fluorescence,Genetic Engineering,Microfluidics,Microscopy, Fluorescence,Models, Biological,Periodicity,Quorum Sensing,Quorum Sensing: genetics,Quorum Sensing: physiology,Time Factors},
month = jan,
number = {7279},
