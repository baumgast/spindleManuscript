keywords = {Breast Neoplasms,Cell Division,Cell Division: drug effects,Cell Division: physiology,Cultured,Dose-Response Relationship,Down-Regulation,Drug,Estrogen,Estrogen Receptor alpha,Estrogen Receptor beta,Estrogen: drug effects,Estrogen: genetics,Estrogen: physiology,Estrogens,Genetic,Genetic: drug effects,Genistein,Genistein: pharmacology,Humans,Isoflavones,Ligands,Messenger,Messenger: drug effects,Messenger: metabolism,Non-Steroidal,Non-Steroidal: pharmacology,Phytoestrogens,Plant Preparations,Proteins,Proteins: metabolism,Quercetin,Quercetin: pharmacology,RNA,Receptors,Transcription,Tumor Cells,Tumor Suppressor Proteins,Up-Regulation},
