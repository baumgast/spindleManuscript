title = {{Ultrasensitivity and noise propagation in a synthetic transcriptional cascade.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=552778\&tool=pmcentrez\&rendertype=abstract},
volume = {102},
year = {2005}
