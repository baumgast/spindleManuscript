issn = {2233-7660},
journal = {Laboratory animal research},
keywords = {1,2,accepted 25 may 2012,belong to the steroid,ers,estrogen,estrogen receptors,hormone superfamily of nuclear,knockout mice,may 2012,nrs,received 3 may 2012,receptors,revised version received 21},
month = jun,
number = {2},
pages = {71--6},
pmid = {22787479},
title = {{Functions and physiological roles of two types of estrogen receptors, ER$\alpha$ and ER$\beta$, identified by estrogen receptor knockout mouse.}},
