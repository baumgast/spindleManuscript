keywords = {Bacillus subtilis,Bacillus subtilis: genetics,Bacillus subtilis: physiology,DNA,DNA: metabolism,Feedback,Gene Expression Regulation, Bacterial,Gene Regulatory Networks,Models, Biological,Transformation, Bacterial},
